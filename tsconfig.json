{
  "compilerOptions": {
    "jsx": "react",
    "lib": [
      "dom",
      "esnext"
    ],
    "target": "es6",
    "module": "esnext",
    "outDir": "./dist",
    "rootDir": "./src",
    "baseUrl": "src",
    "strict": true,
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "esModuleInterop": false,
    "allowJs": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": [
    "src"
  ]
}
